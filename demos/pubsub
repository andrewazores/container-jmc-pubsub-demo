connect jmx-publisher:9091;
dump pub 30 jdk.SocketRead:enabled=true,jdk.SocketWrite:enabled=true,jdk.PhysicalMemory:enabled=true,jdk.CPULoad:enabled=true,jdk.ThreadCPULoad:enabled=true;

connect jmx-subscriber:9091;
dump sub 30 jdk.SocketRead:enabled=true,jdk.SocketWrite:enabled=true,jdk.PhysicalMemory:enabled=true,jdk.CPULoad:enabled=true,jdk.ThreadCPULoad:enabled=true;
wait-for sub;
wait-for-download sub;

connect jmx-publisher:9091;
wait-for pub;
wait-for-download pub;